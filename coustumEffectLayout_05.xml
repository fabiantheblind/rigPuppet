<Effect matchname="tmnpuppet" name="tmn puppet">
	<Group name="headcontrol=headcontrol">
			<Slider name="eyes=eyes" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100"/>
			<Slider name="mouth=mouth" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100"/>
			<Slider name="head=head" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100"/>
			<Point name="headposition=headposition" default_x="0" default_y="0"/>
			<Angle name="headrotate=headrotate" default="0"/>
	</Group>
		<Group name="bodymovement=bodymovement">
			<Group name="legs=legs">
				<Slider name="left=left" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100"/>
				<Point name="positionleft=position left" default_x="0" default_y="0"/>
				<Angle name="rotateleft=rotate left" default="0"/>
				<Slider name="right=right" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100"/>
				<Angle name="rotateright=rotate right" default="0"/>
				<Point name="positionright=position right" default_x="0" default_y="0"/>
			</Group>
			<Group name="arms=arms">
				<Slider name="front=front" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100"/>
				<Angle name="rotatefront=rotate front" default="0"/>
				<Point name="positionfront=position front" default_x="0" default_y="0"/>
				<Slider name="behind=behind" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100"/>
				<Point name="positionbehind=position behind" default_x="0" default_y="0"/>
				<Angle name="rotatebehind=rotate behind" default="0"/>
			</Group>
			<Group name="body=body">
				<Slider name="body=body" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100"/>
				<Angle name="bodyrotate=bodyrotate" default="0"/>
				<Point name="bodyposition=bodyposition" default_x="0" default_y="0"/>
			</Group>
		</Group>
		<Group name="meta=meta">
				<Checkbox name="mirrorpuppet=mirrorpuppet" default="false"/>
				<Slider name="slider=slider" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100"/>
				<Point name="point=point" default_x="0" default_y="0"/>
				<Angle name="angle=angle" default="0"/>
				<Color name="color=color" default_red="128" default_green="128" default_blue="128"/>
				<Layer name="layer=layer" default_self="true"/>
		</Group>
</Effect>